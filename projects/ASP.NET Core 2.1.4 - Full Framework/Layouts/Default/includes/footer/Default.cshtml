@if (ViewData["layoutDefault"] != null)
{
    @* JQuery *@
    <script src="~/lib/jquery/dist/jquery.min.js" asp-append-version="true"></script>

    @* bootstrap *@
    <script src="~/lib/popper.js/dist/umd/popper.min.js" asp-append-version="true"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.min.js" asp-append-version="true"></script>

    @* fontawesome *@
    <script src="~/lib/Font-Awesome/svg-with-js/js/fontawesome-all.min.js" asp-append-version="true"></script>
    
    @* Layout js minified *@
    <script src="~/components/layouts/Default/js/min.js" asp-append-version="true"></script>
}
